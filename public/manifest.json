{
  "name": "Poly Pass",
  "description": "Real Secure Password Manager",
  "version": "0.3",
  "manifest_version": 3,
  "permissions": [
    "tabs",
    "activeTab",
    "storage"
  ],
  "content_scripts": [
    {
      "matches": [
        "<all_urls>"
      ],
      "js": [
        "content_scripts.js"
      ],
      "run_at": "document_end"
    }
  ],
  "background": {
    "service_worker": "background.js"
  },
  "host_permissions": [
    "*://*/*"
  ],
  "action": {
    "default_icon": "assets/images/icon.png",
    "default_popup": "index.html",
    "default_title": "WA Secure Extension"
  }
}